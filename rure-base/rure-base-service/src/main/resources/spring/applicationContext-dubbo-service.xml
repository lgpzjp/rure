<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="rure-base-rpc-service"/>

    <dubbo:registry address="zookeeper://192.168.1.243:2181?backup=192.168.1.242:2181,192.168.1.241:2181"/>

    <dubbo:protocol name="dubbo" port="20883"/>

    <dubbo:monitor protocol="registry"/>

    <bean id="userService" class="com.bdth.rure.base.rpc.service.impl.UserServiceImpl"/>
    <dubbo:service interface="com.bdth.rure.base.api.UserService" ref="userService" timeout="100000"/>

</beans>

